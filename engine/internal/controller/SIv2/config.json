{
	"engineId": "ea0ada2a-7571-4aa5-9172-b5a7d989b041",
	"segmentDuration": "2s",
	"leaveSegmentFiles": false,
	"saveRawAsset": false,

	"chunking": {
		"chunkOverlapDuration": "0s",
		"cache": {
			"region": "us-east-1",
			"bucket": "dev-chunk-cache"
		}
	},

	"frameExtraction": {
		"extractFramesPerSec": 1.0,
		"framesAsPNG": false,
		"thumbnailRate": 60.0,
		"thumbnailWidth": 480
	},

	"assetStorage": {
		"usePresignedS3Urls": true,
		"timeout": "1m"
	},

	"supportedTextMimeTypes": [
		"application/msword",
		"application/vnd.openxmlformats-officedocument.wordprocessingml.document",
		"application/pdf",
		"message/rfc822",
		"application/vnd.ms-outlook",
		"text/plain",
		"text/plain; charset=utf-8",
		"text/html; charset=utf-8",
		"application/vnd.ms-powerpoint",
		"text/rtf",
		"application/vnd.ms-excel",
		"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
		"application/vnd.openxmlformats-officedocument.presentationml.presentation",
		"application/json",
		"application/ttml+xml",
		"application/xml",
		"text/xml",
		"text/csv"
	],

	"veritoneAPI": {
		"baseURL": "https://api.aws-dev.veritone.com",
		"graphQLEndpoint": "/v3/graphql",
		"timeout": "30s",
		"assetTimeout": "1m",
		"debug": false
	},

	"messaging": {
		"kafka": {
			"brokers": ["kafka:9092"],
			"producerMaxRetries": 10,
			"producerTimeout": "10s"
		}
	},
	"writeTimeout": "1m",
	"globalTimeout": "5m"
}
